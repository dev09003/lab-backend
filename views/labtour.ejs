    <style>
        .hidden {
            display: none;
        }
        :root{
    --font: sans;
    --color:white;
    --cardcolor:#4a5568;
    --bgc:gray;
}

*{
    background-color: #101921;
}

.container{
    padding:70px;
    margin: 20px;
}

.container h1{
    color:var(--color);
    font-family: var(--font);
    font-size: xx-large;
    font-weight:900;
}

.container h2{
    margin-left: 450px;
}

.container p{
    color: var(--color);
    font-family: var(--font);
    margin: 30px;
    padding-bottom: 5px;
}

.container h2{
    color:var(--color);
    font-family: var(--font);
    font-size: xx-large;
    font-weight:900;
    margin-bottom: 20px;
}

.cardcontainer {
    display: grid;
    grid-template-columns: repeat(3, 1fr); /* 3 columns, each taking an equal fraction of the space */
    gap: 1rem; /* Space between cards */
    justify-content: center; /* Center the grid horizontally */
    align-items: center; /* Center the grid items vertically */
}

.card {
    width: 400px; /* Adjust width to fill the grid cell */
    height: 600px; /* Fixed height for cards */
    background-color: var(--cardcolor);
    border-radius: 10px;
    box-shadow: 0px 10px 10px 0px black;
    overflow: auto;
    scrollbar-width: 0;
    transition: transform 0.3s ease;
    scrollbar-width: none;
    scrollbar-color: red;
}

.card:hover{
    transform: scale(1.05);
}
.card img{
    width: 90%;
    height:300px;
    border-radius: 10px;
    display: block;
    object-fit: cover;
    font-family: var(--font);
    margin: 20px;
    position: sticky;
}


.card p{
    overflow: hidden;
    flex-grow: 0;
    background-color:var(--cardcolor);
    color: black;
}

.header{
    display: flex;
    flex-direction: row;
    gap:150px;
}

.dropdown{
    color: var(--color);
    font-family: var(--font);
}

.labnames{
    border: white solid 3px;
    border-radius: 9px;
    height: 30px;
}

.chooselab{
    font-size: 18px;
}

.search input{
    border: white solid 2px;
    border-radius: 10px;
    height: 38px;
    color: wheat;
} 

.search button{
    border: black solid 3px;
    border-radius: 10px;
    height: 36px;
    color: rgb(255, 0, 0);
    background-color: yellowgreen;
    width: 60px;
}

    </style>
</head>
